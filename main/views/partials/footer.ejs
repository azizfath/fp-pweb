<div class="bg-dark">
    <div class="container">
        <div class="footer-container-top text-white">
            <div class="footer-content">
                <h4>Product</h4>
                <% product.forEach((element,index)=>{ %>
                    <a href="#" class="nav-link fw-light fs-6">Up to <%= element.bandwidth%></a>
                    <% }) %>
            </div>
            <div class="footer-content">
                <h4>FiberKu</h4>
                <a href="/index" class="nav-link fw-light fs-6">Home</a>
                <a href="/about" class="nav-link fw-light fs-6">About Us</a>
                <a href="/product" class="nav-link fw-light fs-6">Product</a>
                <a href="/contact" class="nav-link fw-light fs-6">Team and Contact</a>
            </div>
            <div class="footer-content">
                <h4>Contact</h4>
                <div class="row align-items-start mt-4">
                    <div class="col-1">
                        <img src="/assets/icons/location.svg" alt="" width="16px">
                    </div>
                    <div class="col-11 fw-light fs-6">
                        <%= contact[0].address %>
                    </div>
                </div>
                <div class="row align-items-start mt-2">
                    <div class="col-1">
                        <img src="/assets/icons/email.svg" alt="" width="16px">
                    </div>
                    <div class="col-11 fw-light fs-6">
                        <%= contact[0].email %>
                    </div>
                </div>
                <div class="row align-items-start mt-2">
                    <div class="col-1">
                        <img src="/assets/icons/phone.svg" alt="" width="16px">
                    </div>
                    <div class="col-11 fw-light fs-6">
                        <%= contact[0].telepon %>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
